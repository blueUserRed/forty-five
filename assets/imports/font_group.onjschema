
$Font {

    $BitmapFont {
        name: string,
        file: string,
        markupEnabled?: boolean
    }

    $FreeTypeFont {
        name: string,
        file: string,
        size: int,
        markupEnabled?: boolean
    }

    $DistanceFieldFont {
        name: string,
        fontFile: string,
        imageFile: string,
        color?: string,
        useMipMapLinearLinear?: boolean,
        markupEnabled?: boolean
    }

}
